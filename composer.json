{
	"name": "mmic/platform-cache-scrubber",
	"type": "platform-extension",
	"description": "Clears Laravel and Platform caches, on-demand",
	"license": "GPLv3",
	"authors": [
		{
			"name": "Medical Mutual Insurance Company of Maine",
			"email": "webmaster@medicalmutual.com"
		}
	],
	"repositories": [
		{
			"type": "composer",
			"url": "https://packages.cartalyst.com"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"cartalyst/composer-installers": "1.2.*",
		"illuminate/support": "~5.0",
		"platform/foundation": "~2.0|~3.0"
	},
	"autoload": {
		"classmap": [
			"database/migrations",
			"database/seeds"
		],
		"psr-4": {
			"Mmic\\CacheScrubber\\": "src/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
